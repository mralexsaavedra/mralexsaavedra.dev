{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard",
    "next"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0,
    "space-before-function-paren": 0,
    "no-new-wrappers": 0,
    "no-undef": 0,
    "no-unused-vars": "off"
  }
}
