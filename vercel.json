{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/AlexanderSaavedra.pdf",
      "destination": "/CV.pdf"
    },
    {
      "source": "/projects",
      "destination": "/about"
    }
  ],
  "headers": [
    {
      "source": "/api/subscribers",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1200, stale-while-revalidate=600"
        }
      ]
    }
  ]
}
