{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/cv",
      "destination": "/alexander-resume-2022.pdf"
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/mralexsaavedra"
    },
    {
      "source": "/linkedin",
      "destination": "https://linkedin.com/in/mralexsaavedra"
    },
    {
      "source": "/github",
      "destination": "https://github.com/mralexsaavedra"
    },
    {
      "source": "/instagram",
      "destination": "https://instagram.com/mralexsaavedra"
    }
  ]
}
