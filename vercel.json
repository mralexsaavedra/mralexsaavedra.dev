{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/AlexanderSaavedra.pdf",
      "destination": "/alexander-resume-2020.pdf"
    },
    {
      "source": "/projects",
      "destination": "/about"
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/mralexsaavedra"
    },
    {
      "source": "/linkedin",
      "destination": "https://linkedin.com/in/mralexsaavedra"
    },
    {
      "source": "/github",
      "destination": "https://github.com/mralexsaavedra"
    }
  ]
}
